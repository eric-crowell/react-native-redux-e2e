{
  "name": "react-native-redux-e2e",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --platform android",
    "build": "detox build --configuration android.emu.release",
    "adb:install": "adb install android/app/build/outputs/apk/androidTest/release/app-release-androidTest.apk",
    "test": "yarn adb:install && detox test --headless --configuration android.emu.release --reuse"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "expo": "~48.0.4",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "^1.4.4",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-native": "0.71.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/react": "^18.0.28",
    "detox": "^20.4.0",
    "jest": "^29.4.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "private": true
}
